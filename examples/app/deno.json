{
  "tasks": {
    "dev": "deno run -A cli.ts dev",
    "start": "deno run -A cli.ts start",
    "build": "deno run -A cli.ts build",
    "init": "deno run -A cli.ts init"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "@timberstack/dhp"
  },
  "imports": {
    "@timberstack/dhp": "npm:@timberstack/dhp"
  }
}
