{
  "tasks": {
    "dev": "deno run -A ../../src/cli.ts dev",
    "start": "deno run -A ../../src/cli.ts start",
    "build": "deno run -A ../../src/cli.ts build",
    "init": "deno run -A ../../src/cli.ts init",
    "test": "deno run -A __tests__/fixture/test_start.ts && deno test -A __tests__/ && deno run -A __tests__/fixture/test_end.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@std/expect": "jsr:@std/expect@^1.0.15",
    "@timberstack/hwr": "../../src/index.ts",
    "@timberstack/vite-plugin-better-manifest": "npm:@timberstack/vite-plugin-better-manifest@^0.0.2",
    "hono": "npm:hono@^4.7.8",
    "vite": "npm:vite@^6.3.4",
    "vitest": "npm:vitest@^3.1.3",
    "@lib/config": "../../src/config.ts",
    "@lib/templates": "../../src/templates.ts"
  }
}
