{
  "compilerOptions": {
    "types": ["deno"],
    "target": "esnext",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
