{
  "compilerOptions": {
    "target": "ES2024",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
