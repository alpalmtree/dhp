{
  "tasks": {
    "release": "deno run -A scripts/release.ts",
    "test": "deno test -A __tests__/tests"
  },
  "lock": false,
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "dhp"
  },
  "imports": {
    "@std/expect": "jsr:@std/expect@^1.0.16",
    "@std/fs": "jsr:@std/fs@^1.0.17",
    "dhp/": "./",
    "dhp/jsx-runtime": "./src/deps/ssx.ts",
    "#src/": "./src/"
  }
}
