{
  "name": "@timberstack/dhp",
  "version": "0.0.1",
  "description": "Deno PHP - A vanilla PHP-like experience for javascript",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "dhp": "./src/cli.ts"
  },
  "scripts": {
    "test": "deno test -A __tests__/tests",
    "cs": "changeset",
    "version": "changeset version",
    "version:test": "changeset version --snapshot charmander",
    "release": "changeset publish",
    "release:test": "changeset publish --no-git-tag --tag charmander",
    "publish:test": "deno run version:test && deno run release:test"
  },
  "keywords": [
    "php",
    "file-based-routing",
    "server side rendering"
  ],
  "author": "Álvaro Palma Mancheño",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.7.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3"
  },
  "files": [
    "./src/",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./jsx": {
      "import": "./src/vendor/hono.ts",
      "types": "./src/vendor/hono.ts"
    },
    "./jsx-runtime": {
      "import": "./src/vendor/hono.ts",
      "types": "./src/vendor/hono.ts"
    },
    "./vite": {
      "import": "./src/ViteHead.tsx",
      "types": "./src/ViteHead.tsx"
    },
    "./cli": {
      "import": "./src/cli.ts",
      "types": "./src/cli.ts"
    },
    "./hono": {
      "import": "./src/vendor/hono.ts",
      "types": "./src/vendor/hono.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
